LOAD CSV
     FROM 'db/csv/reviews.csv' (id, product_id, rating, date, summary, body, recommend, reported, reviewer_name, reviewer_email, response, helpfulness)
     INTO postgresql:///SDC?reviews (id, product_id, rating, date, summary, body, recommend, reported, reviewer_name, reviewer_email, response, helpfulness)

     WITH
     fields terminated by ',';